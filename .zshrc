# Oh My Zsh
export ZSH="/home/victor/.oh-my-zsh"
source $ZSH/oh-my-zsh.sh

# Theme
ZSH_THEME="gentoo"

# Update without asking
DISABLE_UPDATE_PROMPT="true"

# Command correction
ENABLE_CORRECTION="true"

# Enabled plugins
plugins=(git)

# Locale
export LANG=en_US.UTF-8

# Prefered editor
export EDITOR=vim

